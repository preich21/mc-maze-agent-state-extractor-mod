{
	"schemaVersion": 1,
	"id": "mc-maze-agent-state-extractor-mod",
	"version": "${version}",
	"name": "Minecraft Maze Agent State Extractor Mod",
	"description": "This mod enables a reinforcement learning agent to control Minecraft and receive a state representation of the players environment. It is intended to be used in a maze scenario.",
	"authors": [
		"Patrick Reich [patrickreich21@gmail.com]",
		"Axel Herrmann [axel-herrmann@gmx.net]"
	],
	"contact": {
		"sources": "https://github.com/preich21/mc-maze-agent-state-extractor-mod"
	},
	"license": "CC0-1.0",
	"icon": "assets/mc-maze-agent-state-extractor-mod/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"de.kfru.ml.McMazeAgentStateExtractorMod"
		],
		"client": [
			"de.kfru.ml.McMazeAgentStateExtractorModClient"
		]
	},
	"mixins": [
		"mc-maze-agent-state-extractor-mod.mixins.json",
		{
			"config": "mc-maze-agent-state-extractor-mod.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.18.4",
		"minecraft": "~1.21.11",
		"java": ">=21",
		"fabric-api": "*"
	}
}